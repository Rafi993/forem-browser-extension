{
  "name": "Forem",
  "version": "0.1.3",
  "description": "For Empowering Community",
  "manifest_version": 2,
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content/content.js"],
      "run_at": "document_start"
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "@forem",
        "strict_min_version": "42.0"
    }
  }
}
